<div class="jumbotron">
  <h1 class='text-center'>PrinterCut</h1>
  <h4 class='text-center'>Find and Share Printers, Scanners, and Fax Machines Near You </h4>
  <br><br><br><br><br><br>
  <%= simple_form_for @user, method: :get, path: users_path, html: {class: 'text-center'} do %>
      <%= label_tag :search, 'Enter your address to search near you:' %>
      <br>
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "Search", name: nil %>
  <% end %>


</div>



